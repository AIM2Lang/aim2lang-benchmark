  <!-- Object properties -->
  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#hasSkill">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#Skill"/>
    <rdfs:label xml:lang="en">has skill</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#level">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#Level"/>
    <rdfs:label xml:lang="en">has level</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#bloomLevel">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#Bloom"/>
    <rdfs:label xml:lang="en">has Bloom level</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#prerequisite">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#AIM"/>
    <rdfs:label xml:lang="en">has prerequisite</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#isPrerequisiteOf">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#AIM"/>
    <owl:inverseOf rdf:resource="http://aim2lang.org/ontology/aim#prerequisite"/>
    <rdfs:label xml:lang="en">is prerequisite of</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#advanced">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#AIM"/>
    <rdfs:label xml:lang="en">advanced successor</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://aim2lang.org/ontology/aim#hasRubricItem">
    <rdfs:domain rdf:resource="http://aim2lang.org/ontology/aim#AIM"/><rdfs:range rdf:resource="http://aim2lang.org/ontology/aim#RubricItem"/>
    <rdfs:label xml:lang="en">has rubric item</rdfs:label>
  </owl:ObjectProperty>

  <!-- Datatype properties -->
  <owl:DatatypeProperty rdf:about="http://aim2lang.org/ontology/aim#position">
