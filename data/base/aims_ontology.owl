  <skos:ConceptScheme rdf:about="http://aim2lang.org/ontology/aim#LevelScheme"><skos:prefLabel xml:lang="en">CEFR levels</skos:prefLabel></skos:ConceptScheme>
  <skos:ConceptScheme rdf:about="http://aim2lang.org/ontology/aim#BloomScheme"><skos:prefLabel xml:lang="en">Bloom taxonomy</skos:prefLabel></skos:ConceptScheme>

