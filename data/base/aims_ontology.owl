  <aim:Level rdf:about="http://aim2lang.org/ontology/aim#A2"><skos:inScheme rdf:resource="http://aim2lang.org/ontology/aim#LevelScheme"/><skos:prefLabel>A2</skos:prefLabel></aim:Level>
  <aim:Level rdf:about="http://aim2lang.org/ontology/aim#B1"><skos:inScheme rdf:resource="http://aim2lang.org/ontology/aim#LevelScheme"/><skos:prefLabel>B1</skos:prefLabel></aim:Level>
  <aim:Level rdf:about="http://aim2lang.org/ontology/aim#B2"><skos:inScheme rdf:resource="http://aim2lang.org/ontology/aim#LevelScheme"/><skos:prefLabel>B2</skos:prefLabel></aim:Level>
  <aim:Level rdf:about="http://aim2lang.org/ontology/aim#C1"><skos:inScheme rdf:resource="http://aim2lang.org/ontology/aim#LevelScheme"/><skos:prefLabel>C1</skos:prefLabel></aim:Level>
  <aim:Level rdf:about="http://aim2lang.org/ontology/aim#C2"><skos:inScheme rdf:resource="http://aim2lang.org/ontology/aim#LevelScheme"/><skos:prefLabel>C2</skos:prefLabel></aim:Level>

