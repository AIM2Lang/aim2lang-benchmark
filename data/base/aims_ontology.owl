    xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:dct="http://purl.org/dc/terms/" xmlns:prov="http://www.w3.org/ns/prov#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:aim="http://aim2lang.org/ontology/aim#" xml:base="http://aim2lang.org/">

  <owl:Ontology rdf:about="http://aim2lang.org/ontology/aim">
    <dct:title xml:lang="en">AIM2Lang Ontology (TBox)</dct:title>
    <owl:versionInfo>0.2.0</owl:versionInfo>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:about="http://aim2lang.org/ontology/aim#AIM"><rdfs:label xml:lang="en">AIM instance</rdfs:label></owl:Class>
  <owl:Class rdf:about="http://aim2lang.org/ontology/aim#Skill"><rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/><rdfs:label xml:lang="en">Skill</rdfs:label></owl:Class>
  <owl:Class rdf:about="http://aim2lang.org/ontology/aim#Level"><rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/><rdfs:label xml:lang="en">CEFR level</rdfs:label></owl:Class>
